{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "types": ["astro/client"],
      "allowSyntheticDefaultImports": true,
  
      "baseUrl": "./src",
      "paths": {
        "@components/*": ["components/*"],
        "@layouts/*": ["layouts/*"],
        "@services/*": ["services/*"],
        "@api/*": ["api/*"]
      }
    }
  }
  
